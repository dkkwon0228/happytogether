<com.ilovecat.happytogether.RegisterBusinessAreaAndBusinessNum.CheckableRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:appmaker="http://schemas.android.com/apk/res-auto/com.ilovecat.happytogether"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:background="#f2f2f2"
    appmaker:checkable="@+id/radio">

  <TextView
      android:id="@+id/lblListItem"
      android:layout_width="match_parent"
      android:layout_height="40dp"
      android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"
      android:textColor="#999999"
      android:textSize="14sp"
      android:text="child item"
      android:gravity="center_vertical"/>

</com.ilovecat.happytogether.RegisterBusinessAreaAndBusinessNum.CheckableRelativeLayout>